<!DOCTYPE html>
<html dir="ltr" lang="en">

@Html.Partial("/Views/Admin/PartialHeader.cshtml")

<body>
    @Html.Partial("/Views/Admin/PartialPreloader.cshtml")
    <div id="main-wrapper" data-layout="vertical" data-navbarbg="skin5" data-sidebartype="full"
         data-sidebar-position="absolute" data-header-position="absolute" data-boxed-layout="full">
        @Html.Partial("/Views/Admin/PartialTopbar.cshtml")
        @Html.Partial("/Views/Admin/PartialSidebar.cshtml")
        <div class="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
                        <div class="white-box">
                            <h3 class="box-title">@RenderBody()</h3>

                        </div>
                    </div>
                </div>
            </div>
            @Html.Partial("/Views/Admin/PartialFooter.cshtml")
        </div>
    </div>
    @Html.Partial("/Views/Admin/PartialScripts.cshtml")
</body>
</html>
